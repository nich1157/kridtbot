    </inertial>
  </link>

  <gazebo reference="left_rear_rev_link">
    <material>Gazebo/Orange</material>
    <mu1 value="3"/>
    <mu2 value="3"/>
    <kp>1000000.0</kp>
    <kd>1.0</kd>
  </gazebo>

