<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">
    <!-- ROS2 Control plugin for Gazebo-->
    <ros2_control name="imu_controller" type="system">
        <sensor name="bno055">
            <param name="frame_id">imu_link</param>
        </sensor>
    </ros2_control>
</robot>